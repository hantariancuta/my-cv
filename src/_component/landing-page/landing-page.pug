-
    var myFullName = 'Ancuta Hantari'
    var myJobTitle = 'starter in web development'
    var socialList = [
        {icon: 'google-plus', url: 'https://yoururlhere.com/'},
        {icon: 'facebook2', url: 'http://facebook.com/hantariancuta'},
        {icon: 'twitter', url: 'http://twitter.com/hantariancuta'},
        {icon: 'youtube', url: 'http://youtube.com/codetap'},
        {icon: 'github', url: 'http://github.com/hantariancuta'},
        {icon: 'stackoverflow', url: 'http://stackoverflow.com/codetap'},
        {icon: 'skype', url: 'http://skype.com/hantariancuta'},
        {icon: 'linkedin', url: 'https://www.linkedin.com/in/ancuta-hantari-57b59825/ '},
        {icon: 'earth', url: 'http://earth.com/codetap'},
        
        


    ]
#landing-page
.landing-page
    .landing-page__wrapper
        h2.landing-page__name=myFullName
        h1.landing-page__title=myJobTitle
        ul.landing-page__social
            each social, key in socialList
                li.landing-page__item
                    label.landing-page__label(for=`message-box-id-${key}`)
                        span(class=`my-cv__icon--${social.icon}`)
        .landing-page__box-body
           input.landing-page__box-radio(type="radio", name="awesome-radio", checked="checked" id="default-message-radio")
           .landing-page__box-message Click the above icons to reveal
           .landing-page__box-header
             .landing-page__box-info Click to visit
             label.landing-page__box-close(for="default-message-radio") &times;
        each social, key in socialList
            input.landing-page__box-radio(type="radio", name="awesome-radio", id=`message-box-id-${key}`)
            .landing-page__box-message
                a.landing-page__box-link(href=social.url, target="_blank")=social.url